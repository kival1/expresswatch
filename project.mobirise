me=\"formPlaceholder\" title=\"Placeholders\" value=\"#ff7140\" condition=\"customStyle\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" value=\"@PROJECT_PATH@/assets/images/background1-1.jpg\" title=\"Image\">\n            <input type=\"color\" value=\"#ffffff\" title=\"Color\" selected>\n            <input type=\"video\" value=\"https://www.youtube.com/watch?v=x9vz-axHndk\" title=\"Video\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Color\" name=\"overlayColor\" value=\"#656565\" condition=\"overlay&&bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay&&bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n<div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n        <div class=\"row row-content justify-content-center\">\n            <div class=\"col-lg-4\">\n                <div class=\"team-left-inner\">\n                    <div class=\"featured-wrapper\">\n                        \n                        <div class=\"team-info-block\">\n                        <h4 class=\"team-designation mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-2\" data-app-selector=\" .team-left-inner .team-info-block.team-designation\" mbr-if=\"showPosition\">&nbsp; &nbsp; &nbsp; &nbsp; We Repair ALL Watches</h4>\n                     </div>\n                        \n            <img src=\"@PROJECT_PATH@/assets/images/fig-3-700x526.jpg\" title alt=\"Pict\">\n                    </div>\n                    <div class=\"team-info-block\">\n                        <h4 class=\"team-designation mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-2\" data-app-selector=\".team-left-inner .team-info-block .team-designation\" mbr-if=\"showPosition\">&nbsp; &nbsp; &nbsp; &nbsp;</h4>\n                      <div class=\"short-description\">\n                          <p class=\"mbr-text mbr-fonts-style mbr-normal\" mbr-theme-style=\"display-5\" data-app-selector=\".team-left-inner .team-info-block .short-description p\">&nbsp;</p>\n                        </div>    \n                    </div>\n                    <div class=\"team-info-block\" mbr-if=\"showContacts\">\n                        <h3 class=\"personal-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".team-left-inner .team-info-block .personal-title\">Plastic</h3>\n                        <div class=\"mbr-list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-list\" data-multiline mbr-list>\n                        <ul class=\"single-team-info\">\n                                <li mbr-if=\"infoCount > 0\"><label>Phone</label>9</li>\n                                <li mbr-if=\"infoCount > 1\"><label>Email</label>mob</li>\n                                <li mbr-if=\"infoCount > 2\"><label>Website</label>mob</li>\n                                <li mbr-if=\"infoCount > 3\"><label>Fax</label>9</li>\n                                <li mbr-if=\"infoCount > 4\"><label>Fax</label>9</li>\n                                <li mbr-if=\"infoCount > 5\"><label>Fax</label>9</li>\n                                <li mbr-if=\"infoCount > 6\"><label>Fax</label>9</li>\n                                <li mbr-if=\"infoCount > 7\"><label>Fax</label>9</li>\n                            </ul>    \n                        </div>\n                      </div>\n                    <div class=\"team-info-block\" mbr-if=\"showSkills\">\n                        <h3 class=\"personal-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".team-left-inner .team-info-block .personal-title\">Skill</h3>\n                        <div class=\"progress_elements\">\n                            <div class=\"progress1 pb-4\">\n                                <div class=\"title-wrap\">\n                                    <div class=\"progressbar-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\">\n                                        <p data-app-selector=\".progressbar-title p, .progress_value\">\n                                            Soft </p>\n                                    </div>\n                                </div>\n                                <progress class=\"progress progress-primary\" value=\"{{progress1}}\" max=\"100\">\n                                </progress>\n                            </div>\n\n                            <div class=\"progress2 pb-4\" mbr-if=\"progressCount > 1\">\n                                <div class=\"title-wrap\">\n                                    <div class=\"progressbar-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\">\n                                        <p data-app-selector=\".progressbar-title p, .progress_value\">\n                                            Hard</p>\n                                    </div>\n                                </div>\n                                <progress class=\"progress progress-primary\" value=\"{{progress2}}\" max=\"100\">\n                                </progress>\n                            </div>\n                            <div class=\"progress3 pb-4\" mbr-if=\"progressCount > 2\">\n                                <div class=\"title-wrap\">\n                                    <div class=\"progressbar-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\">\n                                        <p data-app-selector=\".progressbar-title p, .progress_value\">\n                                            Interface</p>\n                        